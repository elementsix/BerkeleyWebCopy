  (define left_wall (- 0 300))
  (define right_wall 300)
  (define top_wall (- 0 300))
  (define bottom_wall 300)
  (define speedx 20)
  (define speedy 11)

  (define (draw_circle posx posy fill_color radius)
    (penup)
    (setposition posx posy)
    (pendown)
    (begin_fill)
    (color fill_color)
    (circle radius)
    (end_fill)
  )

  (define (main count posx posy fill_color radius speedx speedy)
      (if (= (modulo count 2) 0) (define fill_color "red") (define fill_color "#f4fc9f"))
      (cond
        ((> (+ posx speedx) right_wall) (main (+ count 1) posx posy fill_color radius (* speedx (- 0 1)) speedy))
        ((< (+ posx speedx) left_wall) (main (+ count 1) posx posy fill_color radius (* speedx (- 0 1)) speedy))
        ((> (+ posy speedy) bottom_wall) (main (+ count 1) posx posy fill_color radius  speedx (* speedy (- 0 1))))
        ((< (+ posy speedy) top_wall)  (main (+ count 1) posx posy fill_color radius speedx (* speedy (- 0 1))))
        (else (draw_circle (+ posx speedx) (+ posy speedy) fill_color radius) (main (+ count 1) (+ posx speedx) (+ posy speedy) fill_color radius  speedx speedy))
      )
  )


